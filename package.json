  },
  "name": "fetch-curl",
  "version": "0.3.5",
  "description": "node-libcurl abstraction to api fetch",
  "main": "src/index.ts",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/marck010/curl-fetch.git"
  },
  "keywords": [
    "node-curl",
    "curl",
    "libcurl",
    "node-libcurl",
    "fetch-curl",
    "fetch-http2"
  ],
  "dependencies": {
    "http-status": "^1.3.2",
    "node-libcurl": "^2.0.1",
    "url": "^0.11.0"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.4",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "clear": "rimraf ./dist",
    "prebuild": "npm run clear",
    "build": "node_modules/typescript/bin/tsc --build ./tsconfig.json",
    "build:watch": "node_modules/typescript/bin/tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "npm run build && node_modules/mocha/bin/mocha --exit dist/tests/test.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs"
  }
}
